{
  "name": "nest-graphql",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "tsc -r tsconfig-paths/register src/main.ts",
    "start:dev": " tsc -w ",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
    "migration:create": "ts-node ./node_modules/typeorm/cli.js migration:create -n"
  },
  "dependencies": {
    "@nestjs/common": "^6.3.2",
    "@nestjs/core": "^6.3.2",
    "@nestjs/graphql": "^6.2.4",
    "@nestjs/platform-express": "^9.2.1",
    "@types/graphql": "^14.2.2",
    "@types/uuid": "^3.4.4",
    "apollo-server-express": "^2.6.7",
    "dotenv": "^8.0.0",
    "graphql": "^14.4.1",
    "graphql-tools": "^4.0.5",
    "pg": "^7.11.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^2.6.3",
    "rxjs": "^6.5.2",
    "type-graphql": "^0.17.4",
    "typeorm": "^0.2.18",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.3.2",
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.10",
    "@types/supertest": "^2.0.7",
    "concurrently": "^4.1.1",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.3.0",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.18.0",
    "typescript": "3.5.2",
    "wait-on": "^3.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
